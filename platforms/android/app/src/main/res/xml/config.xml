    <allow-navigation href="http://192.168.43.185:8100" />
    <allow-navigation href="http://192.168.1.19:8100" />
    <allow-navigation href="http://192.168.1.10:8100" />
    <allow-navigation href="http://192.168.1.33:8100" />
